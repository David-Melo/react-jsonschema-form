{
  "name": "@react-jsonschema-form/bootstrap",
  "version": "2.0.0",
  "description": "Bootstrap UI for React JSON Schema Form",
  "license": "MIT",
  "main": "build/index.js",
  "module": "lib/index.js",
  "variable": "FormTheme",
  "scripts": {
    "start": "cross-env NODE_ENV=development babel src --out-dir lib --watch",
    "build:lib": "cross-env NODE_ENV=production babel src --out-dir lib",
    "build:umd": "neutrino build --use ../../.neutrinorc.js",
    "build": "npm run build:lib && npm run build:umd",
    "icons": "svgr -d src/components/icons svg --icon"
  },
  "peerDependencies": {
    "react": "^16.4.0"
  },
  "dependencies": {
    "source-map-support": "^0.5.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "cross-env": "^5.1.6",
    "neutrino": "^8.2.3",
    "react": "^16.4.0"
  }
}
