{
  "name": "@react-schema-form/utils-compatibility",
  "version": "1.0.0",
  "description": "React Schema Form Utils - compatibility tests",
  "license": "MIT",
  "main": "build/index.js",
  "module": "lib/index.js",
  "variable": "ReactSchemaFormUtilsCompatibility",
  "scripts": {
    "start": "cross-env NODE_ENV=development babel src --out-dir lib --watch",
    "build:lib": "cross-env NODE_ENV=production babel src --out-dir lib",
    "build:umd": "neutrino build --use ../../.neutrinorc.lib.js",
    "build": "npm run build:lib && npm run build:umd"
  },
  "devDependencies": {
    "@neutrinojs/library": "^8.2.3",
    "neutrino": "^8.2.3"
  }
}
