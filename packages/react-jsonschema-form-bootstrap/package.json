{
  "name": "react-jsonschema-form-bootstrap",
  "version": "1.0.0",
  "description": "Bootstrap UI for React JSON Schema Form library",
  "homepage": "https://github.com/mozilla-services/react-jsonschema-form#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mozilla-services/react-jsonschema-form.git"
  },
  "author": "Matej Mazur <matej.mazur@mail.com>",
  "license": "MIT",
  "main": "build/index.js",
  "module": "lib/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development babel src --out-dir lib --watch",
    "build:lib": "cross-env NODE_ENV=production babel src --out-dir lib",
    "build:umd": "neutrino build",
    "build": "npm run build:lib && npm run build:umd",
    "icons": "svgr -d src/components/icons svg --icon"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.js": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@neutrinojs/library": "^8.2.3",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.1.6",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.8.2",
    "husky": "^1.0.0-rc.8",
    "lint-staged": "^7.1.3",
    "neutrino": "^8.2.3",
    "prettier-eslint-cli": "^4.7.1",
    "react": "15.6.2",
    "react-dom": "^15.0.0",
    "react-jsonschema-form": "https://github.com/MatejMazur/react-jsonschema-form#ui-agnostic",
    "svgr": "^1.10.0"
  }
}
