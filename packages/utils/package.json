{
  "name": "@rjsform/utils",
  "version": "1.0.0",
  "description": "RJSForm utils",
  "main": "build/index.js",
  "libName": "RJSFUtils",
  "license": "MIT",
  "module": "lib/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development babel src --out-dir lib --watch",
    "build:umd": "neutrino build --use ../../.neutrinorc.lib.js",
