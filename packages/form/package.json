{
  "name": "@rjsform/form",
  "version": "1.0.0",
  "description": "React JSON Schema Form component",
  "main": "build/index.js",
  "libName": "RJSFormComponent",
  "license": "MIT",
  "module": "lib/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development babel src --out-dir lib --watch",
