{
  "name": "@rjsform/ui-bootstrap",
  "version": "1.0.0",
  "description": "RJSForm components with Bootstrap UI",
  "main": "build/index.js",
  "libName": "RJSFBootstrap",
  "license": "MIT",
  "module": "lib/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development babel src --out-dir lib --watch",
    "build:umd": "neutrino build --use ../../.neutrinorc.lib.js",
    "build:lib": "cross-env NODE_ENV=production babel src --out-dir lib",
    "build": "npm run build:lib && npm run build:umd",
    "icons": "svgr -d src/components/icons svg --icon"
  },
  "devDependencies": {
    "svgr": "^1.10.0"
  },
  "peerDependencies": {
    "react": "^16.4.0"
  },
  "dependencies": {
    "@rjsform/utils": "^1.0.0",
    "prop-types": "^15.6.1"
  }
}
