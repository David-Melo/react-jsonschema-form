{
  "name": "@rjsform/samples",
  "version": "1.0.0",
  "description": "Samples for playgrounds",
  "main": "build/index.js",
  "libName": "RJSFSamples",
  "license": "MIT",
  "module": "lib/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development babel src --out-dir lib --watch",
    "build:umd": "neutrino build --use ../../.neutrinorc.lib.js",
    "build:lib": "cross-env NODE_ENV=production babel src --out-dir lib",
    "build": "npm run build:lib && npm run build:umd"
  },
  "peerDependencies": {
    "react": "^16.4.0"
  }
}
